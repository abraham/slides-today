{
  "name": "slides-today",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/slides-today/stats-es2015.json",
    "build": "ng build --prod",
    "build:development": "npm run build --prod='false'",
    "build:scripts": "cd scripts && tsc",
    "build:stats": "npm run build --stats-json",
    "bundlesize": "bundlesize",
    "clean": "rm -rf dist && rm -rf scripts/dist",
    "deploy": "ng deploy",
    "e2e": "ng e2e",
    "fix": "concurrently npm:fix:*",
    "fix:ng": "ng lint --fix",
    "fix:prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "get-tweet": "ts-node --project scripts/tsconfig.json scripts/get-tweet.ts",
    "id": "ts-node --project scripts/tsconfig.json scripts/id.ts",
    "lighthouse": "firebase emulators:exec \"lhci autorun\"",
    "lint": "concurrently npm:lint:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --allowed-hosts npm --validate-https",
    "lint:ng": "ng lint",
    "lint:prettier": "prettier --check \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "prebuild": "npm run clean",
    "prebuild:scripts": "cp -n example.twitter.json .twitter.json || true; cp -n example.firebase-adminsdk.json .firebase-adminsdk.json || true",
    "serve": "npm run build && npm run start:firebase",
    "start": "ng serve --port 5000 --hmr",
    "start:firebase": "firebase emulators:start",
    "test": "TS_NODE_PROJECT=features/tsconfig.json cucumber-js",
    "test:features": "start-server-and-test start:firebase http://localhost:5000 test",
    "update-tweets": "ts-node --project scripts/tsconfig.json scripts/update-tweets.ts"
  },
  "dependencies": {
    "@angular/animations": "11.1.2",
    "@angular/cdk": "11.1.1",
    "@angular/common": "11.1.2",
    "@angular/compiler": "11.1.2",
    "@angular/core": "11.1.2",
    "@angular/fire": "6.1.4",
    "@angular/forms": "11.1.2",
    "@angular/material": "11.1.1",
    "@angular/platform-browser": "11.1.2",
    "@angular/platform-browser-dynamic": "11.1.2",
    "@angular/router": "11.1.2",
    "@angular/service-worker": "11.1.2",
    "@firebase/app": "0.6.14",
    "@justinribeiro/lite-youtube": "0.9.1",
    "@schuchard/prettier": "4.0.0",
    "@webcomponents/webcomponentsjs": "2.5.0",
    "autotrack": "2.4.1",
    "clipboard-polyfill": "3.0.2",
    "firebase": "8.2.6",
    "first-input-delay": "0.1.3",
    "github-repository": "0.4.0",
    "material-components-web": "9.0.0",
    "ng-lazyload-image": "9.1.0",
    "ngx-skeleton-loader": "2.6.2",
    "node-package": "1.4.0",
    "rxjs": "6.6.3",
    "tslib": "^2.1.0",
    "twitter-status": "0.5.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1101.2",
    "@angular-devkit/build-angular": "0.1101.3",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "11.1.3",
    "@angular/compiler-cli": "11.1.2",
    "@angular/language-service": "11.1.2",
    "@lhci/cli": "0.7.0",
    "@testing-library/dom": "^7.29.4",
    "@types/chai": "4.2.14",
    "@types/cucumber": "6.0.1",
    "@types/jasmine": "3.6.3",
    "@types/jasminewd2": "2.0.8",
    "@types/material-components-web": "1.0.0",
    "@types/node": "10.17.51",
    "@types/puppeteer": "5.4.3",
    "@types/twit": "2.2.28",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "bundlesize": "0.18.1",
    "chai": "4.3.0",
    "codelyzer": "6.0.1",
    "concurrently": "5.3.0",
    "cucumber": "6.0.5",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.6.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-react": "latest",
    "firebase-admin": "9.4.2",
    "firebase-tools": "9.3.0",
    "fuzzy": "0.1.3",
    "inquirer": "7.3.3",
    "inquirer-autocomplete-prompt": "1.3.0",
    "jasmine": "3.6.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lockfile-lint": "4.3.7",
    "pptr-testing-library": "^0.6.4",
    "prettier": "2.2.1",
    "prettier-plugin-package": "1.3.0",
    "protractor": "~7.0.0",
    "puppeteer": "5.2.1",
    "sass": "1.32.6",
    "start-server-and-test": "1.12.0",
    "stylelint": "13.9.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-sass-guidelines": "7.1.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "stylelint-selector-bem-pattern": "2.1.0",
    "ts-node": "9.1.1",
    "twit": "2.2.11",
    "twitter-d": "0.4.0",
    "typescript": "4.1.3",
    "webpack-bundle-analyzer": "4.4.0"
  },
  "bundlesize": [
    {
      "path": "dist/slides-today/*main.*.js",
      "maxSize": "125 kB"
    },
    {
      "path": "dist/slides-today/*home.*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "dist/slides-today/*components.*.js",
      "maxSize": "50 kB"
    },
    {
      "path": "dist/slides-today/!(*main|*home|*components).*.js",
      "maxSize": "30 kB"
    }
  ]
}
