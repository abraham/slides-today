{
  "name": "slides-today",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/slides-today/stats-es2015.json",
    "build:development": "npm run build --prod='false'",
    "build:scripts": "cd scripts && tsc",
    "build:stats": "npm run build --stats-json",
    "build": "ng build --prod",
    "bundlesize": "bundlesize",
    "clean": "rm -rf dist && rm -rf scripts/dist",
    "deploy": "ng deploy",
    "e2e": "ng e2e",
    "get-tweet": "ts-node --project scripts/tsconfig.json scripts/get-tweet.ts",
    "id": "ts-node --project scripts/tsconfig.json scripts/id.ts",
    "lighthouse": "firebase emulators:exec \"lhci autorun\"",
    "lint:features": "tslint --config tslint.json 'features/**/*.ts'",
    "lint:lockfile": "lockfile-lint --path package-lock.json --allowed-hosts npm --validate-https",
    "lint:ng": "ng lint",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently npm:lint:*",
    "prebuild:scripts": "cp -n example.twitter.json .twitter.json || true; cp -n example.firebase-adminsdk.json .firebase-adminsdk.json || true",
    "prebuild": "npm run clean",
    "serve": "npm run build && npm run start:firebase",
    "start:firebase": "firebase emulators:start",
    "start": "ng serve --port 5000",
    "test:features": "start-server-and-test start:firebase http://localhost:5000 test",
    "test": "TS_NODE_PROJECT=features/tsconfig.json cucumber-js",
    "update-tweets": "ts-node --project scripts/tsconfig.json scripts/update-tweets.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.2",
    "@angular/cdk": "9.2.1",
    "@angular/common": "9.1.2",
    "@angular/compiler": "9.1.2",
    "@angular/core": "9.1.2",
    "@angular/fire": "6.0.0",
    "@angular/forms": "9.1.2",
    "@angular/material": "9.2.1",
    "@angular/platform-browser": "9.1.2",
    "@angular/platform-browser-dynamic": "9.1.2",
    "@angular/router": "9.1.2",
    "@angular/service-worker": "9.1.2",
    "@firebase/app": "0.6.2",
    "@justinribeiro/lite-youtube": "0.7.1",
    "@webcomponents/webcomponentsjs": "2.4.3",
    "autotrack": "2.4.1",
    "clipboard-polyfill": "2.8.6",
    "firebase": "7.14.1",
    "first-input-delay": "0.1.3",
    "github-repository": "0.4.0",
    "intl-list-format": "1.0.3",
    "material-components-web": "5.1.0",
    "ng-lazyload-image": "7.1.0",
    "ngx-skeleton-loader": "1.2.7",
    "node-package": "1.4.0",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "twitter-status": "0.5.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.901.3",
    "@angular-devkit/build-angular": "0.901.3",
    "@angular/cli": "9.1.3",
    "@angular/compiler-cli": "9.1.2",
    "@angular/language-service": "9.1.2",
    "@lhci/cli": "0.3.12",
    "@types/chai": "4.2.11",
    "@types/cucumber": "6.0.1",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/material-components-web": "1.0.0",
    "@types/node": "12.12.31",
    "@types/puppeteer": "2.0.1",
    "@types/twit": "2.2.24",
    "bundlesize": "0.18.0",
    "chai": "4.2.0",
    "codelyzer": "5.2.2",
    "concurrently": "5.1.0",
    "cucumber": "6.0.5",
    "firebase-admin": "8.10.0",
    "firebase-tools": "8.1.1",
    "fuzzy": "0.1.3",
    "inquirer": "7.1.0",
    "inquirer-autocomplete-prompt": "1.0.2",
    "jasmine": "3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.1",
    "karma": "5.0.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "lockfile-lint": "4.2.2",
    "pptr-testing-library": "0.5.0",
    "protractor": "6.0.0",
    "puppeteer": "2.1.1",
    "sass": "1.26.3",
    "start-server-and-test": "1.11.0",
    "stylelint": "13.3.3",
    "stylelint-config-sass-guidelines": "7.0.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.17.1",
    "stylelint-selector-bem-pattern": "2.1.0",
    "ts-node": "8.9.0",
    "tslint": "6.1.1",
    "twit": "2.2.11",
    "twitter-d": "0.4.0",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "3.7.0"
  },
  "engines": {
    "node": "12"
  },
  "bundlesize": [
    {
      "path": "dist/slides-today/*main-es2015.*.js",
      "maxSize": "130 kB"
    },
    {
      "path": "dist/slides-today/*components-es2015.*.js",
      "maxSize": "50 kB"
    },
    {
      "path": "dist/slides-today/!(*main|*components)-es2015.*.js",
      "maxSize": "25 kB"
    }
  ]
}
