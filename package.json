{
  "name": "slides-today",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/slides-today/stats-es2015.json",
    "bundlesize": "bundlesize",
    "prebuild": "npm run clean",
    "build": "ng build",
    "build:prod": "npm run build -- --prod",
    "build:stats": "npm run build -- --prod --stats-json",
    "build:scripts": "cd scripts && tsc",
    "clean": "rm -rf dist && rm -rf scripts/dist",
    "prebuild:scripts": "cp -n example.twitter.json .twitter.json || true; cp -n example.firebase-adminsdk.json .firebase-adminsdk.json || true",
    "deploy": "ng deploy",
    "e2e": "ng e2e",
    "lint": "ng lint && npm run lint:stylelint && npm run lint:lockfile",
    "lint:stylelint": "stylelint src/**/*.scss",
    "lint:lockfile": "# TODO: Enable when @justinribeiro/lite-youtube switches back to NPM. lockfile-lint --path package-lock.json --allowed-hosts npm --validate-https",
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "npm run build:prod && serve --single dist/slides-today",
    "test": "npm run lint && npm run build:scripts && npm run build && npm run build:prod",
    "get-tweet": "ts-node --project scripts/tsconfig.json scripts/get-tweet.ts",
    "id": "ts-node --project scripts/tsconfig.json scripts/id.ts",
    "update-tweets": "ts-node --project scripts/tsconfig.json scripts/update-tweets.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.1",
    "@angular/cdk": "9.0.0",
    "@angular/common": "9.0.1",
    "@angular/compiler": "9.0.1",
    "@angular/core": "9.0.1",
    "@angular/fire": "6.0.0-rc.1",
    "@angular/forms": "9.0.1",
    "@angular/material": "9.0.0",
    "@angular/platform-browser": "9.0.1",
    "@angular/platform-browser-dynamic": "9.0.1",
    "@angular/router": "9.0.1",
    "@angular/service-worker": "9.0.1",
    "@firebase/app": "0.5.4",
    "@justinribeiro/lite-youtube": "github:abraham/lite-youtube#typescript",
    "@webcomponents/webcomponentsjs": "2.4.1",
    "autotrack": "2.4.1",
    "clipboard-polyfill": "2.8.6",
    "firebase": "7.8.2",
    "first-input-delay": "0.1.3",
    "github-repository": "0.3.0",
    "intl-list-format": "1.0.3",
    "material-components-web": "4.0.0",
    "ng-lazyload-image": "7.0.1",
    "ngx-skeleton-loader": "1.2.2",
    "node-package": "1.3.0",
    "rxjs": "6.5.4",
    "tslib": "1.10.0",
    "twitter-status": "0.4.1",
    "zone.js": "0.10.2"
  },
  "_devDependencies": "TODO: Remove picomatch when stylelint is updated",
  "devDependencies": {
    "@angular-devkit/architect": "0.900.2",
    "@angular-devkit/build-angular": "0.900.2",
    "@angular/cli": "9.0.2",
    "@angular/compiler-cli": "9.0.1",
    "@angular/language-service": "9.0.1",
    "@types/jasmine": "3.5.4",
    "@types/jasminewd2": "2.0.8",
    "@types/material-components-web": "1.0.0",
    "@types/node": "12.12.21",
    "@types/twit": "2.2.23",
    "bundlesize": "0.18.0",
    "codelyzer": "5.2.1",
    "firebase-admin": "8.9.2",
    "firebase-tools": "7.13.0",
    "fuzzy": "0.1.3",
    "inquirer": "7.0.4",
    "inquirer-autocomplete-prompt": "1.0.2",
    "jasmine": "3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.2",
    "lockfile-lint": "3.0.18",
    "node-sass": "4.13.1",
    "picomatch": "2.2.1",
    "protractor": "6.0.0",
    "serve": "11.3.0",
    "stylelint": "13.2.0",
    "stylelint-config-sass-guidelines": "7.0.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.14.2",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "twit": "2.2.11",
    "twitter-d": "0.4.0",
    "typescript": "3.7.5",
    "webpack-bundle-analyzer": "3.6.0"
  },
  "engines": {
    "node": "12"
  },
  "bundlesize": [
    {
      "path": "dist/slides-today/*main-es2015.*.js",
      "maxSize": "135 kB"
    },
    {
      "path": "dist/slides-today/!(*main)-es2015.*.js",
      "maxSize": "40 kB"
    }
  ]
}
