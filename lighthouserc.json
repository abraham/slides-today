{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "staticDistDir": "./dist/slides-today/",
      "isSinglePageApplication": true,
      "url": [
        "http://localhost:4200/"
      ]
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "unused-css-rules": "warn",
        "dom-size": "warn"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
